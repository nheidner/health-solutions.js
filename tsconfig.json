{
    "compilerOptions": {
        "module": "commonjs",
        "target": "esnext",
        "jsx": "preserve",
        "lib": ["dom", "esnext"],
        "strict": true,
        "noEmit": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "noUnusedLocals": false,
        "allowJs": true,
        "noImplicitAny": false
    },
    "include": ["src/**/*tsx", "src/**/*.ts", "*.ts"]
}
